{
    "name": "herrera-io/silex-template",
    "description": "A Silex service provider for Template.",
    "keywords": ["silex", "template"],
    "homepage": "http://herrera-io.github.com/php-silex-template",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Herrera",
            "email": "kevin@herrera.io",
            "homepage": "http://kevin.herrera.io"
        }
    ],
    "support": {
        "issues": "https://github.com/herrera-io/php-silex-template/issues"
    },
    "require": {
        "php": ">=5.3.3",
        "herrera-io/template": "1.*"
    },
    "require-dev": {
        "silex/silex": "*@dev"
    },
    "autoload": {
        "psr-0": {
            "Herrera\\Template": "src/lib"
        }
    },
    "config": {
        "vendor-dir": "src/vendors"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
